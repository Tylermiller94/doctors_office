<h1>Your Current Lists</h1>

<% if @doctors.any?() %>
  <h3>Here are your Doctors:</h3>

  <ul>
    <% @doctors.each() do |doctor| %>
      <li><a href="/doctors/<%= doctor.id()%>"><%= doctor.name() %></a> </li>
    <% end %>
  </ul>

<% else %>

  <p>There are no doctors</p>

<% end %>
<div class="container">
    <a href="/"><button class="btn">Home</button></a>
</div>
